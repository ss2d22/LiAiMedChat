{
  "name": "limedaichatserver",
  "version": "1.0.0",
  "description": "server for the limedaichat application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node src/scripts/loadTextBooks.ts && nodemon src/index.ts",
    "generate-docs-json": "ts-node src/utils/swaggerOptions.ts",
    "convert-json-to-yaml": "ts-node src/utils/JsonToYaml.ts",
    "generate-docs": "npm run generate-docs-json && npm run convert-json-to-yaml",
    "load-vectors": "ts-node src/scripts/loadTextBooks.ts",
    "build": "tsc && tsc-alias",
    "load-vectors-prod": "node dist/scripts/loadTextBooks.js",
    "start": "node dist/scripts/loadTextBooks.js && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ss2d22/LiAiMedChat.git"
  },
  "keywords": [
    "server",
    "ai",
    "express"
  ],
  "author": "Sriram Sundar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ss2d22/LiAiMedChat/issues"
  },
  "homepage": "https://github.com/ss2d22/LiAiMedChat#readme",
  "dependencies": {
    "@langchain/community": "^0.3.1",
    "@langchain/openai": "^0.3.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "faiss-node": "^0.5.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pickleparser": "^0.2.1",
    "socket.io": "^4.8.0",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "tsc-alias": "^1.8.10",
